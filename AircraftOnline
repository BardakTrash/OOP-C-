class City
{
private:
	int id;
	int s;
	map<int, string, int> cities;
public:
	City(int cityid, int distanse[], string citys[]) : id{cityid}
	{
		for (int i = 0; i < citys->size(); i++)
		{
			cities[i] = citys[i];
		}

	};
	ostream& operator << (ostream& out)
	{
		out << id;
		return out;
	}
};
